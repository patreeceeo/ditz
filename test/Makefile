
MY_PATH=..
include ../common.mk
MY_OBJS=$(COMMON_OBJS:%=$(MY_PATH)/%)
MY_INCLUDE_PATH=$(MY_PATH)/$(COMMON_INCLUDE_PATH)
MY_SOURCE_PATH=$(MY_PATH)/$(COMMON_SOURCE_PATH)
CXXFLAGS=-I$(MY_INCLUDE_PATH) -I$(MY_SOURCE_PATH)
CFLAGS=-I$(MY_INCLUDE_PATH) -I$(MY_SOURCE_PATH)

all: index_adaptor
	

index_adaptor: index_adaptor.o $(MY_OBJS) $(COMMON_LIBS)
	$(COMMON_LINK_RECIPE)
